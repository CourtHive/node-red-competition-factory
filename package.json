{
  "name": "node-red-competition-factory",
  "version": "0.0.9",
  "description": "node-red node wrapping Competition Factory mocksEngine",
  "scripts": {
    "release": "np",
    "start": "tsdx watch",
    "test": "tsdx test",
    "lint": "tsdx eslint src",
    "public": "cp -r ./src/public/ ./dist/",
    "build": "tsdx build && yarn public",
    "prepare": "tsdx build && yarn public",
    "doc:publish": "cd documentation && GIT_USER=CourtHive USE_SSH=true yarn deploy"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "examples"
  ],
  "np": {
    "contents": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CourtHive/node-red-competition-factory.git"
  },
  "keywords": [
    "TODS",
    "tournament",
    "tournaments",
    "competition",
    "tennis",
    "node-red"
  ],
  "author": "Charles Allen",
  "private": false,
  "jest": {
    "watchPathIgnorePatterns": [
      "generated"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CourtHive/node-red-competition-factory/issues"
  },
  "homepage": "https://github.com/CourtHive/node-red-competition-factory#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "tods-competition-factory": "0.6.31"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.2",
    "@babel/plugin-proposal-optional-chaining": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-jest": "26.6.3",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "prettier-eslint": "12.0.0",
    "tsdx": "0.14.1",
    "tslib": "2.2.0",
    "typescript": "4.3.2"
  },
  "node-red": {
    "nodes": {
      "competition-factory": "dist/index.js"
    }
  }
}
