{
  "name": "node-red-mocks-engine",
  "version": "0.0.1",
  "description": "node-red node wrapping Competition Factory mocksEngine",
  "scripts": {
    "release": "np",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx eslint src",
    "prepare": "tsdx build",
    "prepublishOnly": "./addVersion $npm_package_version && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "doc:publish": "cd documentation && GIT_USER=CourtHive USE_SSH=true yarn deploy"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "np": {
    "contents": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CourtHive/node-red-mocks-engine.git"
  },
  "keywords": [
    "TODS",
    "tournament",
    "tournaments",
    "competition",
    "tennis"
  ],
  "author": "Charles Allen",
  "private": false,
  "jest": {
    "watchPathIgnorePatterns": [
      "generated"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CourtHive/node-red-mocks-engine/issues"
  },
  "homepage": "https://github.com/CourtHive/node-red-mocks-engine#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "tods-competition-factory": "^0.5.60"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.12",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.39",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "26.6.3",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "tsdx": "0.14.1",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  }
}
